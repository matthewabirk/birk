if(add0.001==TRUE) exp(mean(log(x+0.001),...)) else exp(mean(log(x),...))
}
geom_mean(0:33)
geom_mean(0:33,add0.001 = T)
geom_mean(-3:33,add0.001 = T)
geom_mean(-3:33,add0.001 = T,ignore_neg = T)
